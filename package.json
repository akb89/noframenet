{
  "name": "noframenet",
  "version": "1.1.2",
  "description": "A set of scripts to import FrameNet XML data to a MongoDB database",
  "keywords": [
    "FrameNet",
    "MongoDB"
  ],
  "author": "Alexandre Kabbach",
  "license": "MIT",
  "scripts": {
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "test": "NODE_ENV=testing ./node_modules/mocha/bin/mocha --compilers js:babel-core/register --require babel-polyfill './tests/**/*.@(js)' --reporter spec",
    "posttest": "npm run lint",
    "clean": "NODE_ENV=production ./node_modules/babel-cli/bin/babel-node.js ./scripts/clean.js",
    "cleandev": "NODE_ENV=development ./node_modules/babel-cli/bin/babel-node.js ./scripts/clean.js",
    "import": "NODE_ENV=production ./node_modules/babel-cli/bin/babel-node.js ./scripts/pipeline.js",
    "importdev": "npm run cleandev && NODE_ENV=development ./node_modules/babel-cli/bin/babel-node.js ./scripts/pipeline.js",
    "fix": "NODE_ENV=production ./node_modules/babel-cli/bin/babel-node.js ./scripts/fix.js",
    "fixdev": "NODE_ENV=development ./node_modules/babel-cli/bin/babel-node.js ./scripts/fix.js"
  },
  "homepage": "https://github.com/akb89/noframenet#readme",
  "repository": {
    "type": "git",
    "url": "git+https://akb89@github.com/akb89/noframenet.git"
  },
  "bugs": {
    "url": "https://github.com/akb89/noframenet/issues"
  },
  "dependencies": {
    "ascii-progress": "^1.0.5",
    "bluebird": "^3.4.1",
    "jsonix": "^2.4.1",
    "mongodb": "^2.2.5",
    "mongoose": "^4.7.0",
    "noframenet-core": "^2.1.0",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "mocha": "^3.2.0",
    "rewire": "^2.5.2"
  }
}
